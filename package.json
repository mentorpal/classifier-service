{
  "name": "mentor-classifier-service",
  "version": "1.0.0",
  "description": "Serverless mentor classifier",
  "devDependencies": {
    "serverless": "^2.72.1",
    "serverless-deployment-bucket": "^1.5.1",
    "serverless-domain-manager": "^6.0.2",
    "serverless-layers": "^2.5.1"
  },
  "scripts": {
    "deploy": "NODE_OPTIONS=--max_old_space_size=4000 sls deploy",
    "deploy:dev": "NODE_OPTIONS=--max_old_space_size=4000 sls deploy -s dev",
    "deploy:qa": "NODE_OPTIONS=--max_old_space_size=4000 sls deploy -s qa",
    "deploy:prod": "NODE_OPTIONS=--max_old_space_size=4000 sls deploy -s prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mentorpal/mentor-classifier-service.git"
  },
  "engines": {
    "node": ">=16.13.0 <17"
  },
  "keywords": [
    "sqs",
    "lambda",
    "mentorpal"
  ],
  "author": "USC ICT",
